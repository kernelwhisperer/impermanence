{
  "name": "impermanence-electron",
  "version": "1.0.0",
  "main": "./build/start.js",
  "description": "An example Electron app",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "start": "electron-forge start",
    "start-electron": "electron .",
    "prebuild": "rm -rf ./build",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "check-types": "tsc --noEmit",
    "check-types:watch": "npm run check-types -- --watch",
    "package": "electron-forge package -p win32 --arch=x64",
    "make": "electron-forge make -p win32 --arch=x64"
  },
  "author": "Daniel Constantin",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "concurrently": "^7.0.0",
    "electron": "^17.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^17.0.0-1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.6.0-dev.20220210"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "wallpaper": "^6.1.1"
  }
}
